<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="portletrel" destination="portlet" minoccurs="0" maxoccurs="*" orderby="field[@name='name']" ordertype="string">
	<title xml:lang="nl">Portlet</title>
	<title xml:lang="en">Portlet</title>

	<item>
		<field ftype="data" name="title">
			<prompt xml:lang="en">Title</prompt>
			<prompt xml:lang="nl">Titel</prompt>
		</field>
		<field fdatapath="field[@name=&apos;name&apos;]" ftype="data">
			<prompt xml:lang="en">Name</prompt>
			<prompt xml:lang="nl">Naam</prompt>
		</field>
	</item>
</list>