<?xml version="1.0"?>
<!DOCTYPE list PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<list role="parameterrel" destination="nodeparameter" minoccurs="0" maxoccurs="1">
	<title xml:lang="nl">Node parameter</title>
	<title xml:lang="en">Node parameter</title>
	
	<item>
		<field ftype="data" name="key">
			<prompt xml:lang="en">Key</prompt>
			<prompt xml:lang="nl">Naam</prompt>
		</field>
		<field ftype="startwizard" wizardname="config/nodeparameter/nodeparameter" inline="true" />
	</item>

	<command name="startwizard" inline="true" wizardname="config/nodeparameter/nodeparameter"
		objectnumber="new">
		<prompt xml:lang="en">New</prompt>
		<prompt xml:lang="nl">Nieuw</prompt>
	</command>

	<action type="create">
		<relation destinationtype="nodeparameter" role="parameterrel"></relation>
	</action>
</list>