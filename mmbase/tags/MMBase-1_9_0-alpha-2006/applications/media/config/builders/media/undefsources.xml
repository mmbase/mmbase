<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="undefsources" maintainer="mediaproject.mmbase.org" version="0" extends="mediasources">
  <names>
    <singular xml:lang="en">Audio or Video source</singular>
    <singular xml:lang="nl">Mediabron (audio of video)</singular>
   
    <plural xml:lang="en">Audio or video sources</plural>
    <plural xml:lang="nl">Mediabronnen (audio of video)</plural>
  </names>
  
  <descriptions>
    <description xml:lang="en">
      Descriptions of the physical media source (not determined wehter it is audio of video). This
      extends mediaousces, because postgresql is not always very good in selecting from tables which have
      extensions.
    </description>
    <description xml:lang="nl">Beschrijving van het daadwerkelijke media bestand</description>
  </descriptions>

</builder>
