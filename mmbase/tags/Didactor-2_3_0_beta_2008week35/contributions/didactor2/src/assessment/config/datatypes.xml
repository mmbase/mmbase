<?xml version="1.0" ?>
<datatypes
    xmlns="http://www.mmbase.org/xmlns/datatypes"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/datatypes http://www.mmbase.org/xmlns/datatypes.xsd">

  <!-- @version $Id: datatypes.xml,v 1.1 2008-07-31 11:48:21 michiel Exp $

  -->

  <datatype base="integer" id="didactor_assessment_troubles">
    <default value="6" />
    <required value="true" />
    <enumeration>
      <entry value="2" xml:lang="nl" display="Geen" />
      <entry value="4" xml:lang="nl" display="Weinig" />
      <entry value="6" xml:lang="nl" display="Niet veel, niet weinig" />
      <entry value="8" xml:lang="nl" display="Veel" />
      <entry value="10" xml:lang="nl" display="Erg veel" />

      <entry value="2" xml:lang="en" display="Nothing" />
      <entry value="4" xml:lang="en" display="Little" />
      <entry value="6" xml:lang="en" display="Not much" />
      <entry value="8" xml:lang="en" display="Much" />
      <entry value="10" xml:lang="en" display="Very much" />
    </enumeration>
  </datatype>

</datatypes>
