<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>

	<!-- ========== Data Sources Definitions =================================== -->
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
	</form-beans>

	<!-- ========== Global Exceptions Definitions =================================== -->
	<!-- ========== Global Forward Definitions =================================== -->
	<!-- ========== Action Mapping Definitions =================================== -->
	<action-mappings>
		<action path="/editors/publish-remote/PublishBuilderAction"
			type="com.finalist.cmsc.publish.forms.PublishBuilderAction" unknown="false" validate="false">
			<forward name="result" path="/editors/publish-remote/index.jsp" redirect="false" />
		</action>
	</action-mappings>

	<message-resources parameter="cmsc-publish-remote" key="PUBLISH-REMOTE" />
</struts-config>