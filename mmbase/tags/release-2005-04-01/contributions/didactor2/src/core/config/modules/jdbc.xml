<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase/ DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<module maintainer="mmbase.org" version="0">

	<!-- <status>
	what is the status of this module options : active or inactive
	-->
	<status>active</status>

	<!-- <classfile>
	-->
	<classfile>org.mmbase.module.database.JDBC</classfile>

	<!-- <properties>
	you can define properties to be used by the classfile (if used) it uses
	a key/value system. Its a optional tag.

	Be sure to fill in the right password for your database!
	-->
	<properties>
		<property name="password">${jdbc.password}</property>
		<property name="port">${jdbc.port}</property>
		<property name="supportclass">${jdbc.supportclass}</property>
		<property name="database">${jdbc.database}</property>
		<property name="user">${jdbc.user}</property>
		<property name="connections">${jdbc.connections}</property>
		<property name="driver">${jdbc.driver}</property>
		<property name="host">${jdbc.host}</property>
		<property name="url">${jdbc.url}</property>
		<property name="querys">${jdbc.querys}</property>
                <property name="probetime">${jdbc.probetime}</property>
	</properties>
</module>
