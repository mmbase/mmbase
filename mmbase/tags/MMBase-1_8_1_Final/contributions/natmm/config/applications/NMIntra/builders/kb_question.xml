<?xml version="1.0"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<!-- kb_question -->
<builder version="1" name="kb_question" maintainer="ernst.bunders" extends="kb_identifiable">
