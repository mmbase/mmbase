<component name="mmbob" templatepath="mmbob" templatebar="education">
  <scope name="component" referid="component">
    <setting name="showlocation" type="boolean" default="true"/>
  </scope>
  <scope name="providers" referid="provider">
    <setting ref="showlocation" />
  </scope>
  <scope name="educations" referid="education">
    <setting ref="showlocation" />
    <setting name="students"     type="boolean" default="true" />
    <setting name="coaches"      type="boolean" default="false" />
  </scope>
