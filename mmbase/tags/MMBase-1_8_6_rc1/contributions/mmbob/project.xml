<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>${basedir}/../../applications/app-base/project.xml</extend>
  <artifactId>mmbase-mmbob</artifactId>     
  <name>MMBob</name>
  <shortDescription>MMBase forum software</shortDescription>
  <description>MMBob is forum software written in MMBase. It is currently in use at kennisnet.nl</description>
  <properties>
    <type>application</type>
    <status>stable</status>
  </properties>
  <dependencies>
    <dependency>
      <groupId>mmbase</groupId>
      <artifactId>mmbase-thememanager</artifactId>
        <version>${mmbase.version}</version>
        <url>http://www.mmbase.org/</url>
        <properties>
          <eclipse.dependency>true</eclipse.dependency>
        </properties>
    </dependency>
    <dependency>
      <groupId>mmbase</groupId>
      <artifactId>mmbase-multilanguagegui</artifactId>
        <version>${mmbase.version}</version>
        <url>http://www.mmbase.org/</url>
        <properties>
          <eclipse.dependency>true</eclipse.dependency>
        </properties>
    </dependency>
    <dependency>
      <groupId>mmbase</groupId>
      <artifactId>mmbase-email</artifactId>
        <version>${mmbase.version}</version>
        <url>http://www.mmbase.org/</url>
        <properties>
          <eclipse.dependency>true</eclipse.dependency>
        </properties>
    </dependency>
  </dependencies>
</project>

