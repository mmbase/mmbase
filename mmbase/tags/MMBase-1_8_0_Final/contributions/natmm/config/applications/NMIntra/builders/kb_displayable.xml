<?xml version="1.0"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<!-- kb_displayable -->
<builder version="1" name="kb_displayable" maintainer="ernst.bunders" extends="object">
    <classfile>Dummy</classfile>
    <searchage>1000</searchage>
    <names>
        <singular xml:lang="nl">kbase displayable</singular>
        <singular xml:lang="en">kbase displayable</singular>
    </names>
    <descriptions>
        <description xml:lang="nl">iets dat in de kbase kan worden getoond</description>
        <description xml:lang="en">something that can be desplayed by the kbase</description>
    </descriptions>
    <properties>
        <property name="max-nodes-from-query">1000</property>
    </properties>
    <fieldlist>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">datum</guiname>
                <guiname xml:lang="en">date</guiname>
                <guitype>eventtime</guitype>
            </gui>
            <editor>
                <positions>
                    <input>3</input>
                    <list>3</list>
                    <search>-1</search>
                </positions>
            </editor>
            <db>
                <name>date</name>
                <type size="11" key="false" state="persistent" notnull="false">INTEGER</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">zichtbaar</guiname>
                <guiname xml:lang="en">visible</guiname>
                <guitype>string</guitype>
            </gui>
            <editor>
                <positions>
                    <input>4</input>
                    <list>4</list>
                    <search>-1</search>
                </positions>
            </editor>
            <db>
                <name>visible</name>
                <type size="50" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
    </fieldlist>
</builder>
