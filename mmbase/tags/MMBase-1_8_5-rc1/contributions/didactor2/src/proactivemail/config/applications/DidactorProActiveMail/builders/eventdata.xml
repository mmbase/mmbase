<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="eventdata" maintainer="didactor.nl" version="1" extends="object">
  <status>active</status>

  <classfile>org.mmbase.module.builders.Dummy</classfile>
  <searchage>1000</searchage>

  <names>
    <!-- singles per language as defined by ISO 639 -->
    <singular xml:lang="en">Event Data</singular>
    <singular xml:lang="nl">Gebeurtenis Data</singular>
  
    <!-- multiple per language as defined by ISO 639 -->
    <plural xml:lang="en">Event Data</plural>
    <plural xml:lang="nl">Gebeurtenis Data</plural>
  </names>

  <descriptions>
    <!-- descriptions per language as defined by ISO 639  -->
    <description xml:lang="en">Data about some events</description>
    <description xml:lang="nl">Data about some events</description>
  </descriptions>

  <fieldlist>
    
    <!-- POS 3 : <field> 'time'  -->
    <field>
      <descriptions>
        <!-- descriptions per language as defined by ISO 639  -->
        <description xml:lang="en">Time of event in miliseconds</description>
        <description xml:lang="nl">Time of eventin miliseconds</description>
      </descriptions>
      <!-- gui related -->
      <gui>
        <guiname xml:lang="en">Time</guiname>
        <guiname xml:lang="nl">Time</guiname>
        <guitype>string</guitype>
      </gui>
      <!-- editor related  -->
      <editor>
        <positions>
          <!-- position in the input area of the editor -->
          <input>3</input>
          <!-- position in list area of the editor -->
          <list>3</list>
          <!-- position in search area of the editor -->
          <search>3</search>
        </positions>
      </editor>
      <!-- database related  -->
      <db>
        <!-- name of the field in the database -->
        <name>timestamp</name>
        <!-- MMBase datatype and demands on it -->
        <type state="persistent" notnull="true" key="false">INTEGER</type>
      </db>
    </field>

    <!-- POS 4 : <field> 'value'  -->
    <field>
      <descriptions>
        <!-- descriptions per language as defined by ISO 639  -->
        <description xml:lang="en">Event Value</description>
        <description xml:lang="nl">Event Value</description>
      </descriptions>
      <!-- gui related -->
      <gui>
        <guiname xml:lang="en">Event Value</guiname>
        <guiname xml:lang="nl">Event Value</guiname>
        <guitype>string</guitype>
      </gui>
      <!-- editor related  -->
      <editor>
        <positions>
          <!-- position in the input area of the editor -->
          <input>4</input>
          <!-- position in list area of the editor -->
          <list>4</list>
          <!-- position in search area of the editor -->
          <search>4</search>
        </positions>
      </editor>
      <!-- database related  -->
      <db>
        <!-- name of the field in the database -->
        <name>value</name>
        <!-- MMBase datatype and demands on it -->
        <type state="persistent" size="20" notnull="true" key="false">STRING</type>
      </db>
    </field>
    
    <!-- POS 5 : <field> 'stringvalue'  -->
    <field>
      <descriptions>
        <!-- descriptions per language as defined by ISO 639  -->
        <description xml:lang="en">String value</description>
        <description xml:lang="nl">String value</description>
      </descriptions>
      <!-- gui related -->
      <gui>
        <guiname xml:lang="en">String value</guiname>
        <guiname xml:lang="nl">Onderwerp</guiname>
        <guitype>string</guitype>
      </gui>
      <!-- editor related  -->
      <editor>
        <positions>
          <!-- position in the input area of the editor -->
          <input>5</input>
          <!-- position in list area of the editor -->
          <list>5</list>
          <!-- position in search area of the editor -->
          <search>5</search>
        </positions>
      </editor>
      <!-- database related  -->
      <db>
        <!-- name of the field in the database -->
        <name>stringvalue</name>
        <!-- MMBase datatype and demands on it -->
        <type state="persistent" size="50" notnull="true" key="false">STRING</type>
      </db>
    </field>

    <!-- POS 6 : <field> 'intvalue'  -->
    <field>
      <descriptions>
        <!-- descriptions per language as defined by ISO 639  -->
        <description xml:lang="en">Integer value</description>
        <description xml:lang="nl">Integer value</description>
      </descriptions>
      <!-- gui related -->
      <gui>
        <guiname xml:lang="en">Integer value</guiname>
        <guiname xml:lang="nl">Integer value</guiname>  
        <guitype>field</guitype>
      </gui>
      <!-- editor related  -->
      <editor>
        <positions>
          <!-- position in the input area of the editor -->
          <input>6</input>
          <!-- position in list area of the editor -->
          <list>6</list>
          <!-- position in search area of the editor -->
          <search>6</search>
        </positions>
      </editor>
      <!-- database related  -->
      <db>
        <!-- name of the field in the database -->
        <name>intvalue</name>
        <!-- MMBase datatype and demands on it -->
        <type state="persistent" notnull="true" key="false">INTEGER</type>
      </db>
    </field>
    
 </fieldlist>
</builder>
