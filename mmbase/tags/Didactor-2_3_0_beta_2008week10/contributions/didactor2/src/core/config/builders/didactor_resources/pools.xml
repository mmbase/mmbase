<?xml version="1.0" encoding="UTF-8"?>
<builder xmlns="http://www.mmbase.org/xmlns/builder"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" extends="object"
         maintainer="www.mmbase.org" name="pools" 
         version="1" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
  <names>
    <singular xml:lang="nl">Poel</singular>
    <singular xml:lang="fr">Regroupement</singular>
    <singular xml:lang="eo">Grupilo</singular>
    <singular xml:lang="en">Pool</singular>
    <plural xml:lang="nl">Poels</plural>
    <plural xml:lang="fr">Regroupements</plural>
    <plural xml:lang="eo">Grupiloj</plural>
    <plural xml:lang="en">Pools</plural>
  </names>
  <descriptions>
    <description xml:lang="nl">Poels, container voor objects</description>
    <description xml:lang="fr">Regroupements, r√©pients pour des objets</description>
    <description xml:lang="eo">Ilo por grupigi objektojn, ujo</description>
    <description xml:lang="en">Pools, containers for objects</description>
  </descriptions>
  <properties/>
  <fieldlist>
    <field name="name">
      <descriptions>
        <description xml:lang="nl">Naam van de poel</description>
        <description xml:lang="en">Name of the pool</description>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Naam</guiname>
        <guiname xml:lang="fr">Nom</guiname>
        <guiname xml:lang="eo">Nomo</guiname>
        <guiname xml:lang="en">Name</guiname>
      </gui>
      <editor>
        <positions>
          <list>1</list>
        </positions>
      </editor>
      <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <type key="false" notnull="false" size="255" state="persistent">STRING</type>
      </db>
    </field>
    <field name="description">
      <descriptions>
        <description xml:lang="nl">Omschrijving van de poel</description>
        <description xml:lang="en">Description of the pool</description>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guiname xml:lang="fr">Description</guiname>
        <guiname xml:lang="eo">Priskribo</guiname>
        <guiname xml:lang="en">Description</guiname>
      </gui>
      <editor>
        <positions>
          <list>2</list>
        </positions>
      </editor>
      <datatype xmlns="http://www.mmbase.org/xmlns/datatypes" base="field"/>
      <db>
        <type key="false" notnull="false" size="2048" state="persistent">STRING</type>
      </db>
    </field>
  </fieldlist>
</builder>
