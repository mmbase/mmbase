<?xml version="1.0" encoding="UTF-8"?>
<project>
    <extend>${basedir}/../../maven-base/project.xml</extend>
    <artifactId>cmsc-enterprise</artifactId>
    <name>Custom CMS container enterprise application</name>
    <shortDescription>CMS container enterprise application</shortDescription>
	<dependencies>
		<dependency>
			<groupId>finalist</groupId>
			<artifactId>cmsc-webapp</artifactId>
			<version>${cmsc.version}</version>
			<type>war</type>
            <properties>
                <ear.bundle>true</ear.bundle>
                <ear.bundle.name>cmsc.war</ear.bundle.name>
				<ear.appxml.war.context-root>/cmsc</ear.appxml.war.context-root>
            </properties>
		</dependency>
	</dependencies>
</project>

