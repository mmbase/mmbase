<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE urlcomposers SYSTEM "http://www.mmbase.org/dtd/urlcomposers_1_0.dtd">
<!-- configuration for org.mmbase.applications.media.urlcomposers.URLComposerFactory -->
<urlcomposers>  
  <!-- This configuration file is used by the Media application example
       It contains Omroep specific stuff. Look at urlcomposers.simple.xml for a more generic example
   -->

 
  <default>org.mmbase.applications.media.urlcomposers.URLComposer</default>

  <urlcomposer format="*" >org.mmbase.applications.media.urlcomposers.MarkupURLComposer</urlcomposer>

  <urlcomposer format="mov" >org.mmbase.applications.media.urlcomposers.URLComposer</urlcomposer>

  <urlcomposer format="ra"  protocol="rtsp">org.mmbase.applications.media.urlcomposers.RealURLComposer</urlcomposer>
  <urlcomposer format="rm"  protocol="rtsp">org.mmbase.applications.media.urlcomposers.RealURLComposer</urlcomposer>
  <urlcomposer format="asf" protocol="mms">org.mmbase.applications.media.urlcomposers.RealURLComposer</urlcomposer>

  <urlcomposer format="ra"  protocol="rtsp">org.mmbase.applications.media.urlcomposers.RamURLComposer</urlcomposer>
  <urlcomposer format="rm"  protocol="rtsp">org.mmbase.applications.media.urlcomposers.RamURLComposer</urlcomposer>
  <urlcomposer format="asf" protocol="mms">org.mmbase.applications.media.urlcomposers.RamURLComposer</urlcomposer>

  <urlcomposer format="ra"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.CgiURLComposer</urlcomposer>
  <urlcomposer format="rm"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.CgiURLComposer</urlcomposer>
  <urlcomposer format="asf" protocol="http">org.mmbase.applications.media.urlcomposers.omroep.CgiURLComposer</urlcomposer>


  <urlcomposer format="ra"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.RealSbURLComposer</urlcomposer>
  <urlcomposer format="rm"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.RealSbURLComposer</urlcomposer>
  <urlcomposer format="asf" protocol="http">org.mmbase.applications.media.urlcomposers.omroep.WmSbURLComposer</urlcomposer>

  <urlcomposer format="ra"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.RealBbURLComposer</urlcomposer>
  <urlcomposer format="rm"  protocol="http">org.mmbase.applications.media.urlcomposers.omroep.RealBbURLComposer</urlcomposer>
  <urlcomposer format="asf" protocol="http">org.mmbase.applications.media.urlcomposers.omroep.WmBbURLComposer</urlcomposer>

</urlcomposers>
