<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase/DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="syncnodes" maintainer="mmbase.org" version="0" extends="object">

  <classfile>Dummy</classfile>
  <searchage>14</searchage>
  <names>
     <!-- singles per language  as defined by RFC xxx -->
     <singular xml:lang="en">Synchronisation Node</singular>
     <singular xml:lang="nl">Synchronisatienode</singular>
     <singular xml:lang="eo">Samignodo</singular>
     <singular xml:lang="it">Sincronizzazione Nodo</singular>
     <!-- multiple per language  as defined by RFC xxx -->
     <plural xml:lang="en">Synchronisation Nodes</plural>
     <plural xml:lang="nl">Synchronisatienodes</plural>
     <plural xml:lang="eo">Samignodoj</plural>
     <plural xml:lang="it">Sincronizzazione Nodi</plural>
  </names>

  <!-- <descriptions>
  small description of the builder for human reading
  -->
  <descriptions>
     <description xml:lang="en">Synchronisation Nodes are used to map remote MMBase nodes to this node</description>
     <description xml:lang="nl">Synchronisatienodes worden gebruikt om remote MMBase nodes te mappen naar lokale nodes</description>
     <description xml:lang="eo">Samignodoj estas uzataj por mapi forajn MMBase-nodoj al lokaj</description>
     <description xml:lang="it">Sincronizzazione Nodi usati per mappare i nodi-MMBase remoti a questo nodo</description>
  </descriptions>

  <!-- <fieldlist>
  defines the different fields in this object, be carefull the order is important
  once defined keep them in the same order.
  If you use number and owner (the 2 default fields, please check the docs for this)
  -->
  <fieldlist>
     <!-- POS 3 : <field> 'exportsource'  -->
     <field>
       <descriptions>
         <!-- descriptions per language as defined by ISO 639  -->
         <description xml:lang="en">Source of this Node, format
         mmbase://sourceservername/sourceapplicationname</description>
         <description xml:lang="nl">Bron van deze node,formaat:
         mmbase://bronservernaam/bronapplicatienaam</description>
       </descriptions>
        <!-- gui related -->
        <gui>
          <guiname xml:lang="en">Export Source</guiname>
          <guiname xml:lang="nl">Exportbron</guiname>
          <guiname xml:lang="eo">Eksportfonto</guiname>
          <guiname xml:lang="it">Esporta fonte</guiname>
          <guitype>string</guitype>
        </gui>
        <!-- editor related  -->
        <editor>
          <positions>
             <!-- position in the input area of the editor -->
             <input>1</input>
             <!-- position in list area of the editor -->
             <list>1</list>
             <!-- position in search area of the editor -->
             <search>1</search>
          </positions>
        </editor>
        <!-- database related  -->
        <db>
          <!-- name of the field in the database -->
          <name>exportsource</name>
          <!-- MMBase datatype and demands on it -->
          <type state="persistent" size="255" notnull="true">STRING</type>
        </db>
     </field>

     <!-- POS 4 : <field> 'timestamp'  -->
     <field>
        <descriptions>
          <!-- descriptions per language as defined by ISO 639  -->
          <description xml:lang="en">Time of the export</description>
          <description xml:lang="nl">Tijd van de export</description>
        </descriptions>
        <!-- gui related -->
        <gui>
          <guiname xml:lang="en">Time Stamp</guiname>
          <guiname xml:lang="nl">Tijdstempel</guiname>
          <guiname xml:lang="eo">Tempstampo</guiname>
          <guiname xml:lang="it">Time stamp</guiname>
          <guitype>integer</guitype>
        </gui>
        <!-- editor related  -->
        <editor>
          <positions>
             <!-- position in the input area of the editor -->
             <input>2</input>
             <!-- position in list area of the editor -->
             <list>2</list>
             <!-- position in search area of the editor -->
             <search>2</search>
          </positions>
        </editor>
        <!-- database related  -->
        <db>
          <!-- name of the field in the database -->
          <name>timestamp</name>
          <!-- MMBase datatype and demands on it -->
          <type state="persistent" notnull="true">INTEGER</type>
        </db>
     </field>

     <!-- POS 5 : <field> 'exportnumber'  -->
     <field>
        <descriptions>
          <!-- descriptions per language as defined by ISO 639  -->
          <description xml:lang="en">Remote number of this node, the
          remotenumber is mapped to the localnumber when importing nodes</description>
          <description xml:lang="nl">Nummer van deze node in de externe cloud, het
          externe nummer wordt gemapped naar het lokale nummer bij een
          import van nodes</description>
        </descriptions>
        <!-- gui related -->
        <gui>
          <guiname xml:lang="en">Export Number</guiname>
          <guiname xml:lang="nl">Exportnummer</guiname>
          <guiname xml:lang="eo">Eksportnumero</guiname>
          <guiname xml:lang="it">Esport Numero</guiname>
          <guitype>integer</guitype>
        </gui>
        <!-- editor related  -->
        <editor>
          <positions>
             <!-- position in the input area of the editor -->
             <input>3</input>
             <!-- position in list area of the editor -->
             <list>3</list>
             <!-- position in search area of the editor -->
             <search>3</search>
          </positions>
        </editor>
        <!-- database related  -->
        <db>
          <!-- name of the field in the database -->
          <name>exportnumber</name>
          <!-- MMBase datatype and demands on it -->
          <type state="persistent" notnull="true">INTEGER</type>
        </db>
     </field>


     <!-- POS 6 : <field> 'localnumber'  -->
     <field>
        <descriptions>
          <!-- descriptions per language as defined by ISO 639  -->
          <description xml:lang="en">Local number of this node, the
          remotenumber is mapped to the localnumber when importing nodes</description>
          <description xml:lang="nl">Lokaal nummer van deze node, het
          externe nummer wordt gemapped naar het lokale nummer bij een
          import van nodes</description>
        </descriptions>
        <!-- gui related -->
        <gui>
          <guiname xml:lang="en">Local Number</guiname>
          <guiname xml:lang="nl">Lokaal Nummer</guiname>
          <guiname xml:lang="eo">Äˆi-numero</guiname>
          <guiname xml:lang="it">Numero Locale</guiname>
          <guitype>integer</guitype>
        </gui>
        <!-- editor related  -->
        <editor>
          <positions>
             <!-- position in the input area of the editor -->
             <input>4</input>
             <!-- position in list area of the editor -->
             <list>4</list>
             <!-- position in search area of the editor -->
             <search>4</search>
          </positions>
        </editor>
        <!-- database related  -->
        <db>
          <!-- name of the field in the database -->
        <name>localnumber</name>
          <!-- MMBase datatype and demands on it -->
          <type state="persistent" notnull="true">NODE</type>
        </db>
     </field>

  </fieldlist>
</builder>
