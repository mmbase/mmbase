<?xml version="1.0"?>
<!DOCTYPE wizard-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<wizard-schema id="responseform">
   <title xml:lang="nl">Dynamisch formulier</title>
   <title xml:lang="en">Dynamic form</title>
   <title xml:lang="zh">动态表单</title>
   <action include="config/responseform/create_responseform.xml" />
   <action include="config/responseform/delete_responseform.xml" />
   <action include="config/responseform/load_responseform.xml" />

   <lists>
      <optionlist include="config/option_lists/boolean.xml" />
      <optionlist include="config/option_lists/inputtypes.xml" />
   </lists>

   <steps>
      <step form-schema="edit_responseform" />
   </steps>

   <form-schema id="edit_responseform" extends="config/responseform/edit_responseform.xml" />
</wizard-schema>
