<?xml version="1.0"?>
<luceneindex xmlns="http://www.mmbase.org/xmlns/luceneindex"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.mmbase.org/xmlns/luceneindex http://www.mmbase.org/xmlns/luceneindex.xsd"
             disableIndexes="comma separated lists of the names of index definitions that must be ignored"
             >
  <!-- example extractors
    <extractor class="org.mmbase.module.lucene.extraction.impl.PDFBoxExtractor" />
    <extractor class="org.mmbase.module.lucene.extraction.impl.SwingRTFExtractor" />
    <extractor class="org.mmbase.module.lucene.extraction.impl.POIWordExtractor" />
  OR:
    <extractor class="org.mmbase.module.lucene.extraction.impl.TextMiningExtractor" />
    <extractor class="org.mmbase.module.lucene.extraction.impl.POIExcelExtractor" />
  -->

  <!--
      Strategies are not additive. You have to specify all strategies in every luceneindex.xml.
      If a certain luceneindex.xml defines no strategies, default will be taken from the 'first' luceindex.xml which _does_ define them.

  -->
  <strategy class="org.mmbase.cache.BetterStrategy" />
  <strategy class="org.mmbase.cache.ConstraintsMatchingStrategy" />

  <!-- no default indices -->


</luceneindex>
