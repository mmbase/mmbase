<?xml version="1.0" encoding="UTF-8"?>
<builder 
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder 
                        http://www.mmbase.org/xmlns/builder.xsd"                        
    name="weblogthemes" maintainer="mmcoder.nl" version="0" extends="object">


  <names>
    <singular xml:lang="en">WebLogTheme</singular>
    <plural xml:lang="en">WebLogThemes</plural>
  </names>

  <descriptions>
    <description xml:lang="en">MMLog a simple weblog system</description>
  </descriptions>

  <fieldlist>

    <field>
      <descriptions>
        <description xml:lang="en">Name</description>
      </descriptions>
      <gui>
        <guiname xml:lang="en">Name</guiname>
      </gui>
      <editor>
        <positions>
          <input>1</input>
          <list>1</list>
          <search>1</search>
        </positions>
      </editor>
      <datatype base="eline" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>name</name>
        <type notnull="true" size="255" state="persistent">STRING</type>
      </db>
    </field>

  </fieldlist>

</builder>
