<?xml version="1.0" encoding="UTF-8"?>
<builder
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder
                        http://www.mmbase.org/xmlns/builder.xsd"
	extends="object" maintainer="finalist.com" name="archive" version="0">

   <status>active</status>

   <!-- <classfile>leeuwnet.Archief</classfile> -->

   <searchage>70</searchage>

   <names>
      <singular xml:lang="nl">archief</singular>
      <singular xml:lang="en">archive</singular>
      <plural xml:lang="nl">archiefs</plural>
      <plural xml:lang="en">archives</plural>
   </names>

   <descriptions>
      <description xml:lang="nl">Archief voor een content element</description>
      <description xml:lang="en">Archive for a content element</description>
   </descriptions>

   <properties>
   </properties>

   <fieldlist>
      <field>
         <descriptions>
            <description xml:lang="nl">Nummer van het content element</description>
            <description xml:lang="en">Number of the content element</description>
         </descriptions>
         <gui>
            <guiname xml:lang="nl">Originele node nummer</guiname>
            <guiname xml:lang="en">Original node number</guiname>
         </gui>
         <editor>
            <positions>
               <input>4</input>
               <list>4</list>
               <search>4</search>
            </positions>
         </editor>
         <db>
            <name>original_node</name>
            <type key="false" notnull="false" size="1" state="persistent">INTEGER</type>
         </db>
      </field>
      <field>
         <descriptions>
            <description xml:lang="nl">Datum</description>
            <description xml:lang="en">Date</description>
         </descriptions>
         <gui>
            <guiname xml:lang="nl">Datum</guiname>
            <guiname xml:lang="en">Date</guiname>
         </gui>
         <editor>
            <positions>
               <input>5</input>
               <list>5</list>
               <search>5</search>
            </positions>
         </editor>
         <db>
            <name>date</name>
            <type key="false" notnull="false" state="persistent">DATETIME</type>
         </db>
      </field>
      <field>
         <descriptions>
            <description xml:lang="nl">De originele data van de het content element</description>
            <description xml:lang="en">The original data of the content element</description>
         </descriptions>
         <gui>
            <guiname xml:lang="nl">node_data</guiname>
            <guiname xml:lang="en">node_data</guiname>
         </gui>
         <editor>
            <positions>
               <input>6</input>
               <list>-1</list>
               <search>-1</search>
            </positions>
         </editor>
         <db>
            <name>node_data</name>
            <type key="false" notnull="false" size="1048576" state="persistent">BYTE</type>
         </db>
      </field>
      <field>
         <descriptions>
            <description xml:lang="nl">Het node nummer van de auteur</description>
            <description xml:lang="en">The number of the author</description>
         </descriptions>
         <gui>
            <guiname xml:lang="nl">auteur</guiname>
            <guiname xml:lang="en">author</guiname>
         </gui>
         <editor>
            <positions>
               <input>7</input>
               <list>-1</list>
               <search>-1</search>
            </positions>
         </editor>
         <db>
            <name>author_number</name>
            <type key="false" notnull="false" state="persistent">INTEGER</type>
         </db>
      </field>
</fieldlist>
</builder>
