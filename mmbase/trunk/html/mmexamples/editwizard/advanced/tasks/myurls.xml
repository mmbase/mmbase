<?xml version="1.0"?>
<!DOCTYPE wizard-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<wizard-schema id="empty">
  <title>Customized URL editwizard (to test startwizard)</title>
  
  <action type="load">
  </action>
  
  <action type="create">
    <object type="urls">
      <field name="url" ftype="string">http://</field>
      <field name="description" ftype="string" />
    </object>
  </action>
  
  <form-schema id="step">
    <title>URL</title>
    <field name="url" />
    <field name="description" />
  </form-schema>
</wizard-schema>
