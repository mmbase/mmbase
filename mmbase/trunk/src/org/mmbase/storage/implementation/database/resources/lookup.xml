<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE database-filters PUBLIC "-//MMBase//DTD database storage lookup 1.0//EN" "http://www.mmbase.org/dtd/database-storage-lookup_1_0.dtd" >
<database-filters>
	<!--
		The database-config is the name referencing the database-config-xml.
		The first filter where all the rules apply, will be selected.
	-->

	<!-- left over jdbc-drivers for mysql -->
	<filter resource="/org/mmbase/storage/implementation/database/resources/mysql.xml">
		<driver-class>org.gjt.mm.mysql.</driver-class>
	</filter>

	<!-- newer version of mysql uses relational? -->
	<filter resource="/org/mmbase/storage/implementation/database/resources/mysql.xml">
		<driver-class>com.mysql.</driver-class>
	</filter>

	<!-- hsql -->
	<filter resource="/org/mmbase/storage/implementation/database/resources/hypersonic.xml">
		<driver-class>org.hsql.</driver-class>
	</filter>

	<!-- hsqldb -->
	<filter resource="/org/mmbase/storage/implementation/database/resources/hsqldb.xml">
		<driver-class>org.hsqldb.</driver-class>
	</filter>

	<!-- postgresql -->
	<filter resource="/org/mmbase/storage/implementation/database/resources/postgresql.xml">
		<driver-class>org.postgresql.</driver-class>
	</filter>

</database-filters>
