<?xml version="1.0" encoding="UTF-8"?>
<builder 
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder 
                        http://www.mmbase.org/xmlns/builder.xsd"                        
    name="forummessagebox" maintainer="mmbase.org" version="0" extends="object">

  <names>
   <singular xml:lang="en">Forum private message box</singular>
   <plural xml:lang="en">Forum private message boxes</plural>
  </names>

  <descriptions>
    <description xml:lang="en">Container for forum private messages</description>
  </descriptions>

  <fieldlist>
    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Naam</guiname>
        <guiname xml:lang="us">Name</guiname>
        <guiname xml:lang="en">Name</guiname>
        <guiname xml:lang="fr">Nom</guiname>
      </gui>
      <editor>
        <positions>
          <input>1</input>
          <list>2</list>
          <search>1</search>
        </positions>
      </editor>
      <datatype base="eline" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name xml:lang="en">name</name>
        <type key="false" notnull="false" size="255" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guiname xml:lang="us">Description</guiname>
        <guiname xml:lang="en">Description</guiname>
        <guiname xml:lang="fr">Description</guiname>
      </gui>
      <editor>
        <positions>
          <input>2</input>
          <list>1</list>
          <search>2</search>
        </positions>
      </editor>
      <datatype base="field" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>description</name>
        <type key="false" notnull="false" size="2048" state="persistent">STRING</type>
      </db>
    </field>

    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="en">editstate</guiname>
      </gui>
      <editor>
        <positions>
          <input>6</input>
          <list>-1</list>
          <search>6</search>
        </positions>
      </editor>
      <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>editstate</name>
        <type key="false" notnull="false" state="persistent">INTEGER</type>
      </db>
    </field>

    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="en">maxmessages</guiname>
      </gui>
      <editor>
        <positions>
          <input>7</input>
          <list>-1</list>
          <search>7</search>
        </positions>
      </editor>
      <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>maxmessages</name>
        <type key="false" notnull="false" state="persistent">INTEGER</type>
      </db>
    </field>

    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="en">maxsize</guiname>
      </gui>
      <editor>
        <positions>
          <input>8</input>
          <list>-1</list>
          <search>8</search>
        </positions>
      </editor>
      <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>maxsize</name>
        <type key="false" notnull="false" state="persistent">INTEGER</type>
      </db>
    </field>

    <field>
      <descriptions>
      </descriptions>
      <gui>
        <guiname xml:lang="en">carboncopymode</guiname>
      </gui>
      <editor>
        <positions>
          <input>9</input>
          <list>-1</list>
          <search>9</search>
        </positions>
      </editor>
      <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes" />
      <db>
        <name>carboncopymode</name>
        <type key="false" notnull="false" state="persistent">INTEGER</type>
      </db>
    </field>

  </fieldlist>
</builder>
