<?xml version="1.0"?>
<!DOCTYPE form-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<form-schema id="edit_stylesheet">
	<title xml:lang="nl">Stylesheet</title>
	<title xml:lang="en">Stylesheet</title>

	<field name="title" dtmaxlength="40">
		<prompt xml:lang="en">Title</prompt>
		<prompt xml:lang="nl">Titel</prompt>
	</field>
	<field name="description" dtmaxlength="1024" ftype="text" rows="3">
		<prompt xml:lang="en">Description</prompt>
		<prompt xml:lang="nl">Omschrijving</prompt>
	</field>
	<field name="resource" dtmaxlength="255" ftype="text" rows="3">
		<prompt xml:lang="en">Resource</prompt>
		<prompt xml:lang="nl">Stylesheet</prompt>
	</field>
</form-schema>