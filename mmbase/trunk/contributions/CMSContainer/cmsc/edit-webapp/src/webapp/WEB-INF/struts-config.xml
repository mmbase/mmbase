<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
          "http://struts.apache.org/dtds/struts-config_1_2.dtd">
<struts-config>

	<!-- ========== Data Sources Definitions =================================== -->
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
	</form-beans>

	<!-- ========== Global Exceptions Definitions =================================== -->
	<!-- ========== Global Forward Definitions =================================== -->
	<!-- ========== Action Mapping Definitions =================================== -->
	<action-mappings>

        <!-- ==== Typedef ==== -->

        <action path="/editors/typedef/TypedefAction"
                type="com.finalist.cmsc.typedef.forms.TypedefAction" scope="session"
                unknown="false" validate="false">
            <forward name="success" path="/editors/typedef/typelist.jsp" redirect="false"/>
        </action>
	</action-mappings>

	<message-resources parameter="cmsc" key="CMSC" />
   <message-resources parameter="cmsc-calendar" key="CALENDAR" />
</struts-config>