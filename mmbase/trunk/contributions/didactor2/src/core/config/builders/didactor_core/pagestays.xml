<?xml version="1.0" ?>
<builder
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd"
    name="pagestays" maintainer="didactor.nl" version="1" extends="object">

  <names>
  </names>

  <descriptions>
  </descriptions>

  <fieldlist>

    <field name="user">
      <gui>
        <guiname xml:lang="en">Person</guiname>
        <guiname xml:lang="nl">Persoon</guiname>
      </gui>
      <editor><positions><list>1</list></positions></editor>
      <datatype base="node" xmlns="http://www.mmbase.org/xmlns/datatypes">
        <required value="false" />
      </datatype>
    </field>

    <field name="content">
      <gui>
        <guiname xml:lang="en">Content object</guiname>
        <guiname xml:lang="nl">Inhoudsobject</guiname>
      </gui>
      <editor><positions><list>2</list></positions></editor>
      <datatype base="node" xmlns="http://www.mmbase.org/xmlns/datatypes" >
        <required value="false" />
      </datatype>
    </field>

    <field name="page">
      <gui>
        <guiname xml:lang="en">Page</guiname>
        <guiname xml:lang="nl">Pagina</guiname>
      </gui>
      <editor><positions><list>3</list></positions></editor>
      <datatype base="eline" xmlns="http://www.mmbase.org/xmlns/datatypes">
        <maxLength value="1024" />
      </datatype>
    </field>

    <field name="duration">
      <gui>
        <guiname xml:lang="en">Stay time (ms)</guiname>
        <guiname xml:lang="nl">Verblijfstijd (ms)</guiname>
      </gui>
      <editor><positions><list>3</list></positions></editor>
      <datatype base="long" xmlns="http://www.mmbase.org/xmlns/datatypes">
        <default value="0" />
        <required value="true" />
      </datatype>
    </field>
  </fieldlist>
</builder>
