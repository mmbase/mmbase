<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" version="1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

	<portlet>
		<description>I18NChannelPortlet</description>
		<portlet-name>i18nchannelportlet</portlet-name>
		<display-name>I18NChannelPortlet</display-name>
		<portlet-class>com.finalist.cmsc.portlets.i18n.I18NChannelPortlet</portlet-class>
		<init-param>
			<name>template.edit_defaults</name>
			<value>ContentChannel.jsp</value>
		</init-param>
		<init-param>
			<name>template.help</name>
			<value>ContentChannel.jsp</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>HELP</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
			<portlet-mode>EDIT_DEFAULTS</portlet-mode>
			<portlet-mode>DELETE</portlet-mode>
		</supports>
		<supported-locale>en</supported-locale>
		<supported-locale>nl</supported-locale>
		<resource-bundle>com.finalist.cmsc.portlets.ContentChannelPortlet</resource-bundle>
		<portlet-info>
			<title>I18NChannelPortlet</title>
			<short-title>I18NChannelPortlet</short-title>
			<keywords>I18NChannel</keywords>
		</portlet-info>
	</portlet>

</portlet-app>