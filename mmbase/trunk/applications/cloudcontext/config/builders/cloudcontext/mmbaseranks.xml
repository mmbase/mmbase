<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="mmbaseranks" maintainer="www.mmbase.org" version="1" extends="object">
  <classfile>org.mmbase.security.implementation.cloudcontext.builders.Ranks</classfile>
  <names>
    <singular xml:lang="en">MMBase Rank</singular>
    <singular xml:lang="nl">MMBase Rang</singular>
    <singular xml:lang="eo">MMBase-Rango</singular>
    <plural xml:lang="en">MMBase Ranks</plural>
    <plural xml:lang="nl">MMBase Rangen</plural>
    <plural xml:lang="eo">MMBase-Rangoj</plural>
  </names>
  <descriptions>
    <description xml:lang="en">
      Ranks used by cloud context security implementation
    </description>
    <description xml:lang="nl">
    </description>
  </descriptions>
  <fieldlist>
    <field>
      <gui>
        <guiname xml:lang="nl">Rang</guiname>
        <guiname xml:lang="eo">Rango</guiname>
        <guiname xml:lang="en">Rank</guiname>
      </gui>
      <editor>
        <positions>
          <input>1</input><list>2</list><search>1</search>
        </positions>
      </editor>
      <db>
        <name>rank</name>
        <type state="persistent" notnull="true" key="true">INTEGER</type>
      </db>
    </field>
    <field>
      <gui>
        <guiname xml:lang="nl">Naam</guiname>
        <guiname xml:lang="eo">Nomo</guiname>
        <guiname xml:lang="en">Name</guiname>
      </gui>
      <editor>
        <positions>
          <input>2</input><list>1</list><search>2</search>
        </positions>
      </editor>
      <db>
        <name>name</name>
        <type state="persistent" size="32" notnull="true" key="true">STRING</type>
      </db>
    </field>
    <field>
      <gui>
        <guiname xml:lang="en">Description</guiname>
        <guiname xml:lang="nl">Omschrijving</guiname>
        <guiname xml:lang="eo">Priskribo</guiname>
        <guitype>field</guitype>
      </gui>
      <editor>
        <positions>
          <input>2</input><list>-1</list><search>2</search>
        </positions>
      </editor>
      <db>
        <name>description</name>
        <type state="persistent" size="1024" notnull="true">STRING</type>
      </db>
    </field>
  </fieldlist>
</builder>
