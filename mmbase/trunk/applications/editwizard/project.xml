<project>
  <extend>${basedir}/../app-base/project.xml</extend>
  <artifactId>mmbase-editwizard</artifactId>
  <name>Editwizards</name>
  <shortDescription>Editwizards</shortDescription>
  <description>
    The Editwizards are the main way of editting and inserting data in MMBase. 
    They allow you to write task-based wizards for data entry. This application provides the required
    tools and software, and some example wizards. 
  </description>
  <properties>
    <type>library</type>
    <status>stable</status>
  </properties>
  <dependencies>
    <dependency>
      <groupId>mmbase</groupId>
      <artifactId>mmbase-dove</artifactId>
      <version>${mmbase.version}</version>
      <url>http://www.mmbase.org/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
  </dependencies>
</project>
