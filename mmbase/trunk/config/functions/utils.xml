<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE functionset PUBLIC "//MMBase - functionset //" "http://www.mmbase.org/dtd/functionset_1_0.dtd">
<functionset>
  <status>active</status>
  <version>6</version>
  <description>
    Some util functions from org.mmbase.util, made accessible thourhg function-framework
  </description>

  <function name="generatePassword">
    <description>
      Generates a password.
    </description>
    <type>class</type>
    <class>org.mmbase.util.PasswordGenerator</class>
    <method>getPassword</method>
    <param name="template" type="String">SSSSSS</param>
    <return type="String" />
  </function>


</functionset>
