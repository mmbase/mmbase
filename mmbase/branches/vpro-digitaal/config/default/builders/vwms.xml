		<db>
			<!-- name of the field in the database -->
			<name>owner</name>
			<!-- MMBase datatype and demands on it -->
			<type state="persistent" notnull="false">VARCHAR</type>
		</db>
	</field>

	<!-- POS 3 : <field> 'name'  -->
