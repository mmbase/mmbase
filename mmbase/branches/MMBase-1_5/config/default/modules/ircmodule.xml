<?xml version="1.0"?>
<!DOCTYPE module PUBLIC "//MMBase - module//" "http://www.mmbase.org/dtd/module.dtd">
<module>

  <!-- <status>
	what is the status of this builder options : active or inactive
  -->
  <status>inactive</status>


  <!-- <classfile>
	main classfile that this module is defined in
  -->
  <classfile>org.mmbase.module.IrcModule</classfile>

  <!-- <properties>
     you can define properties to be used by the classfile (if used) it uses
     a key/value system. Its a optional tag.
   -->
  <properties>
	<property name="ircServer">irc.xs4all.nl</property>
	<property name="channel">mmbase2</property>
	<property name="nickname">mmbase2</property>
	<property name="refreshUrl">www.mmbase.org:8080</property>
	<property name="refreshTime">200</property>
  </properties>
</module>
