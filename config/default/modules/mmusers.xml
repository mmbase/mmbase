<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase/ DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<module>

  <!-- <status>
    what is the status of this builder options : active or inactive
  -->
  <status>inactive</status>


  <!-- <classfile>
    main classfile that this module is defined in
  -->
  <classfile>org.mmbase.module.MMUsers</classfile>

  <!-- <properties>
     you can define properties to be used by the classfile (if used) it uses
     a key/value system. Its a optional tag.
   -->
  <properties>
    <property name="from">bugs@mmbase.org</property>
    <property name="returnpath">bugs@mmbase.org</property>
    <property name="subject">Login info</property>
    <property name="bodybegin">your login en and password.</property>
    <property name="bodylogin">Login: </property>
    <property name="bodypasswd">Password: </property>
    <property name="bodyend">Have fun with the mmbase.org site, keep this mail !!!</property>
  </properties>
</module>
