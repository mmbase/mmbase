<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE application PUBLIC "//MMBase - application//" "http://www.mmbase.org/dtd/application.dtd">
<application name="BridgeTest" maintainer="mmbase.org" version="1" auto-deploy="false">

  <neededbuilderlist>
    <builder maintainer="mmbase.org" version="0">aa</builder>
    <builder maintainer="mmbase.org" version="0">bb</builder>
    <builder maintainer="mmbase.org" version="0">cc</builder>
  </neededbuilderlist>

  <neededreldeflist>
    <reldef source="related" target="related" direction="bidirectional" guisourcename="Related" guitargetname="Related" />
    <reldef source="posrel" target="posrel" direction="bidirectional" guisourcename="PosRel(Parent)" guitargetname="PosRel(Child)" />
  </neededreldeflist>

  <allowedrelationlist>
    <relation from="aa" to="bb" type="related" />
    <relation from="bb" to="cc" type="posrel" />
  </allowedrelationlist>

  <datasourcelist>
    <datasource builder="aa" path="BridgeTest/aa.xml" />
    <datasource builder="bb" path="BridgeTest/bb.xml" />
    <datasource builder="cc" path="BridgeTest/cc.xml" />
  </datasourcelist>

  <relationsourcelist>
    <relationsource builder="insrel" path="BridgeTest/insrel.xml" />
    <relationsource builder="posrel" path="BridgeTest/posrel.xml" />
  </relationsourcelist>

  <contextsourcelist>
    <contextsource path="BridgeTest/backup.xml" type="depth" goal="backup"/>
  </contextsourcelist>

  <description>

The BridgeTest application contains the needed builders and
relations to run the org.mmbase.bridgetest tests.

  </description>

  <install-notice>
  </install-notice>

</application>
