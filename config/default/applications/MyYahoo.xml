<application name="MyYahoo" maintainer="mmbase.org" version="1" auto-deploy="false">

	<neededbuilderlist>
	  <builder maintainer="mmbase.org" version="12">pools</builder>
	  <builder maintainer="mmbase.org" version="11">urls</builder>
	</neededbuilderlist>	

	<neededreldeflist>
	  <reldef source="related" target="related" direction="bidirectional" guisourcename="Related" guitargetname="Related" />
