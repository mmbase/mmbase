	<allowedrelationlist>
	  <relation from="pools" to="pools" type="related" />
	  <relation from="pools" to="urls" type="related" />
	</allowedrelationlist>

	<datasourcelist>
	  <datasource path="MyYahoo/pools.xml" />
	  <datasource path="MyYahoo/urls.xml" />
	</datasourcelist>

	<relationsourcelist>
	  <relationsource path="MyYahoo/insrel.xml" />	
	</relationsourcelist>
</application>
