<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase//DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<module>
	<status>inactive</status>
  <classfile>org.mmbase.module.gui.flash.MMFlash</classfile>

  <properties>
    
    <!-- <property name="generatortemppath">
    Path where the temporary files will be created (*.sws files)
    -->
    <property name="generatortemppath">/tmp/flash/</property>
    
    <!-- <property name="generatorpath">
    Path where the flash generator can be found, not nessecary when
    the program can be found in a directory of the PATH variable.
    -->
    <property name="generatorpath"></property>
    
    <!-- <property name="generatorprogram">
    Name of the generator program, which should be used. Normally would
    be "swift-generator" from http://www.swift-tools.com/swift-generator.html
    -->
    <property name="generatorprogram">swift-generator</property>
  </properties>
</module>
