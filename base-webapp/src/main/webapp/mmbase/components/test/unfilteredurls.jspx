<mm:content
    xmlns:mm="http://www.mmbase.org/mmbase-taglib-2.0"
    xmlns:c="http://java.sun.com/jsp/jstl/core"
    xmlns:jsp="http://java.sun.com/JSP/Page"
    expires="0"
    type="text/html" language="en">
  <html>
    <head>
      <title>Testing urls outside filtered urls</title>
    </head>
    <body >
      <mm:import externid="x" />
      <p>
        <mm:link absolute="context">
          <c:choose>
            <c:when test="${empty x}">
              ${_ eq '/mmbase/components/test/unfilteredurls.jspx' ? 'ok' : 'NOT ok'}: '${_}'
            </c:when>
            <c:otherwise>
              ${_ eq '/mmbase/components/test/unfilteredurls.jspx?x=X' ? 'ok' : 'NOT ok'}: '${_}'
            </c:otherwise>
          </c:choose>
        </mm:link>
      </p>
    </body>
  </html>
</mm:content>
