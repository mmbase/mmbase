<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//MMBase//DTD module config 1.0//EN" "http://www.mmbase.org/dtd/module_1_0.dtd">
<!--
		The MMAdmin module contains procedures, callable from jsp or SCAN, which allow for
		administration tasks, such as installing applications, monitoring cache performance,
		or viewing module and builder properties.
-->
<module>
  <!--
    This module is necessary to administrate MMbase, and should normally be active.
  -->
  <status>active</status>

  <class>org.mmbase.module.tools.MMAdmin</class>


  <properties>
    <!--
        Comma separated list of application names, which are not to be autodeployed, even if they may be autodeploy.
    -->
    <property name="ignored-auto-deploy"></property>
  </properties>

</module>
