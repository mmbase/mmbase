<?xml version="1.0" encoding="UTF-8"?>
<component
    name="crontab"
    defaultblock="frontend"
    xmlns="http://www.mmbase.org/xmlns/component"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/component http://www.mmbase.org/xmlns/component.xsd">


  <description xml:lang="nl">MMBase crontab</description>
  <description xml:lang="en">MMBase crontab</description>

  <block 
      type="mmbase.admin"
      name="admin"
      mimetype="text/html" >
    <description xml:lang="en">Administrates the mmbase crontab</description>      
    <body jsp="/mmbase/crontab/component.jspx">
      <param name="kick" />
      <param name="interrupt" />
      <param name="threadno" />
      <param name="stop" />
      <param name="start" />
      <param name="reload" />
    </body>
  </block>

</component>
