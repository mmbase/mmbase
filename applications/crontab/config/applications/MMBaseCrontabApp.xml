    <reldef guisourcename="related" builder="insrel" guitargetname="related" source="related" direction="BIDIRECTIONAL" target="related"/>
  </neededreldeflist>
  <allowedrelationlist>
    <relation type="related" to="mmservers" from="cronjobs"/>
  </allowedrelationlist>
  <datasourcelist />
  <relationsourcelist/>
  <contextsourcelist />
