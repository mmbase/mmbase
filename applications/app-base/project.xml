<project>
  <extend>../../maven-base/project.xml</extend>
  <artifactId>mmbase-application</artifactId>
  <repository>
    <connection>scm:cvs:pserver:guest@cvs.mmbase.org:/var/cvs:applications/${pom.artifactId.substring(7)}</connection>
    <developerConnection>scm:cvs:ext:${maven.username}@cvs.mmbase.org:/var/cvs:applications/${pom.artifactId.substring(7)}</developerConnection>
    <url>http://cvs.mmbase.org/viewcvs/applications/${pom.artifactId.substring(7)}</url>
  </repository>
    <dependencies>
        <dependency>
            <groupId>mmbase</groupId>
            <artifactId>mmbase</artifactId>
            <version>${mmbase.version}</version>
            <url>http://www.mmbase.org/</url>
            <properties>
		        <eclipse.dependency>true</eclipse.dependency>
            </properties>
        </dependency>
    </dependencies>
</project>