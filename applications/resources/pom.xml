  </description>

  <artifactId>mmbase-resources</artifactId>
  <name>MMBase Resources</name>
  <packaging>war</packaging>
  <version>${mmbase.version}</version>


  <dependencies>
    <dependency>
      <groupId>javax.media</groupId>
      <artifactId>jai-core</artifactId>
