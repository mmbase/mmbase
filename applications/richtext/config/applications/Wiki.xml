<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC "-//MMBase//DTD application config 1.1//EN" "http://www.mmbase.org/dtd/application_1_1.dtd">
<application name="Wiki" maintainer="mmbase.org" version="2" auto-deploy="false">

  <requirements>
    <requires name="RichTextBasics" maintainer="mmbase.org" version="1" />
    <requires name="Versioning"     maintainer="mmbase.org" version="1" />
  </requirements>

  <neededbuilderlist>
    <builder maintainer="mmbase.org" version="0">wikiobjects</builder>
    <builder maintainer="mmbase.org" version="0">wikiobjects_versions</builder>
  </neededbuilderlist>

  <neededreldeflist>
  </neededreldeflist>

  <allowedrelationlist>
    <relation from="wikiobjects"    to="urls"        type="idrel" /><!-- inline urls -->
    <relation from="wikiobjects"    to="images"      type="idrel" /><!-- inline images -->
    <relation from="wikiobjects"    to="attachments" type="idrel" /><!-- inline images -->
  </allowedrelationlist>

  <datasourcelist />

  <relationsourcelist />

  <contextsourcelist />

  <description><![CDATA[]]></description>

  <install-notice><![CDATA[]]></install-notice>

</application>
