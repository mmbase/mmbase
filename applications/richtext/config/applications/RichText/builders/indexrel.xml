<?xml version="1.0" ?>
<builder 
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder 
                        http://www.mmbase.org/xmlns/builder.xsd"                        
    name="indexrel" maintainer="mmbase.org" version="0" extends="posrel">
  <names>
    <singular xml:lang="en">Index relation</singular>
    <singular xml:lang="nl">Indexrelatie</singular>
    <plural xml:lang="en">Index Relations</plural>
    <plural xml:lang="nl">Indexrelaties</plural>
  </names>
  <fieldlist>
    <field>
      <gui>
        <guiname xml:lang="nl">Index</guiname>
        <guiname xml:lang="en">Index</guiname>
      </gui>
      <editor>
        <positions>
          <input>4</input>
          <list>4</list>
          <search>4</search>
        </positions>
      </editor>
      <db>
        <name>index</name>
        <type key="false" notnull="false" size="255" state="persistent">STRING</type>
      </db>
    </field>
    <field>
      <gui>
        <guiname xml:lang="nl">Wortel</guiname>
        <guiname xml:lang="en">Root</guiname>
      </gui>
      <editor>
        <positions>
          <input>5</input>
          <list>5</list>
          <search>6</search>
        </positions>
      </editor>
      <db>
        <name>root</name>
        <type key="false" notnull="false" state="persistent">NODE</type>
      </db>
    </field>
  </fieldlist>
</builder>
