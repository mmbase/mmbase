<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE urlcomposer SYSTEM "http://www.mmbase.org/dtd/urlcomposer_1_0.dtd">

<urlcomposer>

	<!-- This will arrange the mimetype mapping according to the format/codec. You can
		use wildcards to map all formats to a certain mime type.
		-->
	<mimemapping>
		<map format="ra" codec="surestream">audio/x-pn-realaudio</map>
		<map format="*" codec="*">audio/x-pn-realaudio</map>

	</mimemapping>

</urlcomposer>
