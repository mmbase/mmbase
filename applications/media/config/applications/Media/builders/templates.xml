<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE builder PUBLIC "-//MMBase/DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<builder name="templates" maintainer="mmbase.org" version="0" extends="object">
  <names>
    <singular xml:lang="nl">Sjabloon</singular>
    <singular xml:lang="en">Template</singular>
    <plural xml:lang="nl">Sjablonen</plural>
    <plural xml:lang="en">Templates</plural>
  </names>
  <descriptions>
    <description xml:lang="nl">Sjablonen. Een sjabloon is een url naar
    een locale file, die iets kan laten zien.</description>
    <description xml:lang="en">Templates</description>
  </descriptions>
 <fieldlist>
	<field>
		<gui>
			<guiname xml:lang="nl">Url</guiname>
			<guiname xml:lang="en">Url</guiname>
			<guitype>string</guitype>
		</gui>
		<editor>
			<positions>
				<input>2</input><list>2</list><search>2</search>
			</positions>
		</editor>
		<db>
			<name>url</name>
			<type state="persistent" size="255" notnull="true" key="false">STRING</type>
		</db>
	</field>

	<field>
		<gui>
			<guiname xml:lang="nl">Naam</guiname>
			<guiname xml:lang="en">Name</guiname>
			<guitype>string</guitype>
		</gui>
		<editor>
			<positions>
				<input>1</input><list>1</list><search>1</search>
			</positions>
		</editor>
		<db>
			<name>name</name>
			<type state="persistent" size="255" notnull="true" key="false">STRING</type>
		</db>
	</field>

	<field>
		<gui>
			<guiname xml:lang="nl">Omschrijving</guiname>
			<guiname xml:lang="en">Description</guiname>
			<guitype>field</guitype>
		</gui>
		<editor>
			<positions>
				<input>2</input><list>1</list><search>2</search>
			</positions>
		</editor>
		<db>
			<name>description</name>
			<type state="persistent" size="255" notnull="true" key="false">STRING</type>
		</db>
	</field>


	<field>
		<gui>
			<guiname xml:lang="nl">Mime-type</guiname>
			<guiname xml:lang="en">Mime-type</guiname>
			<guitype>field</guitype>
		</gui>
		<editor>
			<positions>
				<input>3</input><list>2</list><search>3</search>
			</positions>
		</editor>
		<db>
			<name>mimetype</name>
			<type state="persistent" size="255" notnull="true" key="false">STRING</type>
		</db>
	</field>

</fieldlist>

</builder>
