<?xml version="1.0"?>
<!DOCTYPE wizard-schema PUBLIC "-//MMBase/DTD editwizard 1.0//EN" "http://www.mmbase.org/dtd/wizard-schema_1_0.dtd">
<!-- this example is similar to the library wizard lib/createurl-->
<wizard-schema id="myurls">
  <title>Customized URL editwizard (to test startwizard)</title>

  <action type="create">
    <object type="urls">
      <field name="name">My site</field>
      <field name="url" >http://</field>
    </object>
  </action>

  <form-schema id="step">
    <title>URL</title>
    <field name="name" />
    <field name="url" />
    <field name="description" />
  </form-schema>

</wizard-schema>
