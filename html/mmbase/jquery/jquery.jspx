<jsp:root
    version="2.0"
    xmlns:jsp="http://java.sun.com/JSP/Page"
    xmlns:mm="http://www.mmbase.org/mmbase-taglib-2.0" >
  <jsp:output omit-xml-declaration="true" />
  <mm:context>
    <mm:import id="javascript" from="request" externid="org.mmbase.jquery" />
    <mm:notpresent referid="javascript">
      <mm:link page="/mmbase/jquery/jquery-1.2.3.min.js">
        <script src="${_}" type="text/javascript"><!-- help msie --></script>
      </mm:link>
      <mm:write session="org.mmbase.jquery" value="yes" />
    </mm:notpresent>
  </mm:context>
</jsp:root>
