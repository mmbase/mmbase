<?xml version="1.0" encoding="UTF-8"?>
<builder xmlns="http://www.mmbase.org/xmlns/builder" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         extends="object" maintainer="didactor.nl" 
         name="agendas" version="2" 
         xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
  <names>
    <singular xml:lang="nl">Agenda</singular>
    <singular xml:lang="en">Agenda</singular>
    <plural xml:lang="nl">Agendas</plural>
    <plural xml:lang="en">Agendas</plural>
  </names>

  <descriptions>
    <description xml:lang="nl">Een agenda bevat items met daar aan mmevents</description>
    <description xml:lang="en">A calander with items, which have an mmevents object related.</description>
  </descriptions>

  <properties>
    <property name="max-nodes-from-query">99999</property>
  </properties>

  <fieldlist>
    <field name="name">
      <descriptions>
        <description xml:lang="nl">Naam van de agenda</description>
        <description xml:lang="en">Name of the agenda</description>
      </descriptions>
      <gui>
        <guiname xml:lang="nl">Naam</guiname>
        <guiname xml:lang="en">Name</guiname>
      </gui>
      <editor>
        <positions>
          <list>1</list>
        </positions>
      </editor>
      <db>
        <type state="persistent" size="255" notnull="true" key="false">STRING</type>
      </db>
    </field>
  </fieldlist>
</builder>
