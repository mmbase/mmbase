<?xml version="1.0" encoding="UTF-8"?>
<builder extends="insrel" maintainer="finalist.com" name="imagerel" version="1"
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
    <names>
        <singular xml:lang="nl">imagerel</singular>
        <singular xml:lang="en">imagerel</singular>
        <singular xml:lang="zh">图片连接</singular>
        <plural xml:lang="nl">imagerel</plural>
        <plural xml:lang="en">imagerel</plural>
        <plural xml:lang="zh">图片连接</plural>
    </names>
    <fieldlist>
        <field name="order">
            <gui>
                <guiname xml:lang="nl">Volgorde</guiname>
                <guiname xml:lang="en">order</guiname>
                <guiname xml:lang="zh">顺序</guiname>
            </gui>
            <editor>
                <positions>
                    <input>4</input>
                </positions>
            </editor>
            <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes"/>
        </field>
        <field name="pos">
            <gui>
                <guiname xml:lang="nl">Positie</guiname>
                <guiname xml:lang="en">pos</guiname>
                <guiname xml:lang="zh">位置</guiname>
            </gui>
            <editor>
                <positions>
                    <input>5</input>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="popup">
            <gui>
                <guiname xml:lang="nl">Popup</guiname>
                <guiname xml:lang="en">popup</guiname>
                <guiname xml:lang="zh">弹出</guiname>
            </gui>
            <editor>
                <positions>
                    <input>6</input>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="legend">
            <gui>
                <guiname xml:lang="nl">Bijschrift</guiname>
                <guiname xml:lang="en">legend</guiname>
                <guiname xml:lang="zh">图例</guiname>
            </gui>
            <editor>
                <positions>
                    <input>7</input>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="width">
            <gui>
                <guiname xml:lang="nl">Breedte</guiname>
                <guiname xml:lang="en">Width</guiname>
                <guiname xml:lang="zh">宽</guiname>
            </gui>
            <editor>
                <positions>
                    <input>8</input>
                </positions>
            </editor>
            <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes"/>
        </field>
        <field name="height">
            <gui>
                <guiname xml:lang="nl">Hoogte</guiname>
                <guiname xml:lang="en">Height</guiname>
                <guiname xml:lang="en">高</guiname>
            </gui>
            <editor>
                <positions>
                    <input>9</input>
                </positions>
            </editor>
            <datatype base="integer" xmlns="http://www.mmbase.org/xmlns/datatypes"/>
        </field>
        <field name="crop">
            <gui>
                <guiname xml:lang="nl">Afsnijden</guiname>
                <guiname xml:lang="en">Crop</guiname>
                <guiname xml:lang="en">裁切</guiname>
            </gui>
            <editor>
                <positions>
                    <input>10</input>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
    </fieldlist>
</builder>
