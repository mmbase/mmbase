<?xml version="1.0" encoding="UTF-8"?>
<builder extends="object" maintainer="finalist.com" name="stylesheet"
    version="2" xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
    <names>
        <singular xml:lang="nl">stylesheet</singular>
        <singular xml:lang="en">stylesheet</singular>
        <singular xml:lang="zh">样式表</singular>
        <plural xml:lang="nl">stylesheet</plural>
        <plural xml:lang="en">stylesheet</plural>
        <plural xml:lang="zh">样式表</plural>
    </names>
    <fieldlist>
        <field name="title">
            <gui>
                <guiname xml:lang="nl">Titel</guiname>
                <guiname xml:lang="en">Title</guiname>
                <guiname xml:lang="zh">标题</guiname>
            </gui>
            <editor>
                <positions>
                    <input>4</input>
                    <list>4</list>
                    <search>4</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <required value="true"/>
                <maxLength value="40"/>
            </datatype>
        </field>
        <field name="description">
            <gui>
                <guiname xml:lang="nl">Omschrijving</guiname>
                <guiname xml:lang="en">Description</guiname>
                <guiname xml:lang="zh">描述</guiname>
            </gui>
            <editor>
                <positions>
                    <input>5</input>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="resource">
            <gui>
                <guiname xml:lang="nl">Stylesheet</guiname>
                <guiname xml:lang="en">Resource</guiname>
                <guiname xml:lang="zh">资源</guiname>
            </gui>
            <editor>
                <positions>
                    <input>6</input>
                    <list>6</list>
                    <search>6</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="255"/>
            </datatype>
        </field>
        <field name="media">
            <gui>
                <guiname xml:lang="nl">Media</guiname>
                <guiname xml:lang="en">Media</guiname>
                <guiname xml:lang="zh">媒介</guiname>
            </gui>
            <editor>
                <positions>
                    <input>7</input>
                    <list>7</list>
                    <search>7</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="30"/>
            </datatype>
        </field>
        <field name="overwriteable">
            <gui>
                <guiname xml:lang="nl">Overwriteable</guiname>
                <guiname xml:lang="en">Overwriteable</guiname>
                <guiname xml:lang="zh">可重写的</guiname>
            </gui>
            <editor>
                <positions>
                    <input>8</input>
                    <list>8</list>
                    <search>8</search>
                </positions>
            </editor>
            <datatype base="yesno" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <required value="true"/>
            </datatype>
        </field>
    </fieldlist>
</builder>
