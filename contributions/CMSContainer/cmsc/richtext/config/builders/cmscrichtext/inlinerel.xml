<?xml version="1.0" encoding="UTF-8"?>
<builder extends="insrel" maintainer="finalist.com" name="inlinerel" version="1"
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
    <class>com.finalist.cmsc.richtext.builders.InlineRel</class>
    <searchage>1000</searchage>
    <names>
        <singular xml:lang="nl">Inlinerel</singular>
        <singular xml:lang="en">Inlinerel</singular>
        <singular xml:lang="zh">内嵌链接</singular>
        <plural xml:lang="nl">Inlinerels</plural>
        <plural xml:lang="en">Inlinerels</plural>
        <plural xml:lang="zh">内嵌链接</plural>
    </names>
    <descriptions>
        <description xml:lang="nl">Inline link relatie</description>
        <description xml:lang="en">Inline link reiation</description>
        <description xml:lang="zh">内嵌链接关系</description>
    </descriptions>
    <fieldlist>
        <field name="referid">
            <gui>
                <guiname xml:lang="nl">ReferentieId</guiname>
                <guiname xml:lang="en">ReferId</guiname>
                <guiname xml:lang="zh">关联Id</guiname>
            </gui>
            <editor>
                <positions>
                    <input>5</input>
                    <list>5</list>
                    <search>5</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <required value="true"/>
                <maxLength value="20"/>
            </datatype>
        </field>
    </fieldlist>
</builder>
