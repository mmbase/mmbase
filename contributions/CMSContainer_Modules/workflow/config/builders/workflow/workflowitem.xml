<?xml version="1.0" encoding="UTF-8"?>
<builder extends="object" maintainer="finalist.com" name="workflowitem"
    version="1" xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
    <names>
        <singular xml:lang="nl">workflowitem</singular>
        <singular xml:lang="en">workflowitem</singular>
        <singular xml:lang="zh">工作流条目</singular>
        <plural xml:lang="nl">workflowitems</plural>
        <plural xml:lang="en">workflowitems</plural>
        <plural xml:lang="zh">工作流条目</plural>
    </names>
    <descriptions>
        <description xml:lang="nl">Workflowitem van een content element</description>
        <description xml:lang="en">Workflowitem for a content element</description>
        <description xml:lang="zh">内容元素的工作条目</description>
    </descriptions>
    <fieldlist>
        <field name="type">
            <descriptions>
                <description xml:lang="nl">Type</description>
                <description xml:lang="en">Type</description>
                <description xml:lang="zh">类型</description>
            </descriptions>
            <gui>
                <guiname xml:lang="nl">Type</guiname>
                <guiname xml:lang="en">Type</guiname>
                <guiname xml:lang="zh">类型</guiname>
            </gui>
            <editor>
                <positions>
                    <input>4</input>
                    <list>4</list>
                    <search>4</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="32"/>
                <enumeration>
                    <entry basename="com.finalist.cmsc.workflow.resources.type"/>
                </enumeration>
            </datatype>
        </field>
        <field name="status">
            <descriptions>
                <description xml:lang="nl">Status</description>
                <description xml:lang="en">Status</description>
                <description xml:lang="zh">状态</description>
            </descriptions>
            <gui>
                <guiname xml:lang="nl">Status</guiname>
                <guiname xml:lang="en">Status</guiname>
                <guiname xml:lang="zh">状态</guiname>
            </gui>
            <editor>
                <positions>
                    <input>5</input>
                    <list>5</list>
                    <search>5</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="32"/>
                <enumeration>
                    <entry basename="com.finalist.cmsc.workflow.resources.status"/>
                </enumeration>
            </datatype>
        </field>
        <field name="remark">
            <descriptions>
                <description xml:lang="nl">Opmerking</description>
                <description xml:lang="en">Remark</description>
                <description xml:lang="zh">标注</description>
            </descriptions>
            <gui>
                <guiname xml:lang="nl">Opmerkingen</guiname>
                <guiname xml:lang="en">Remark</guiname>
                <guiname xml:lang="zh">标注</guiname>
            </gui>
            <editor>
                <positions>
                    <input>6</input>
                    <list>6</list>
                    <search>6</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="4096"/>
            </datatype>
        </field>
        <field name="creationdate" state="system">
            <gui>
                <guiname xml:lang="nl">Aangemaakt</guiname>
                <guiname xml:lang="en">Creationdate</guiname>
                <guiname xml:lang="zh">创建时间</guiname>
            </gui>
            <editor>
                <positions>
                    <input>7</input>
                    <list>7</list>
                    <search>7</search>
                </positions>
            </editor>
            <datatype base="created" xmlns="http://www.mmbase.org/xmlns/datatypes"/>
        </field>
        <field name="lastmodifieddate" state="system">
            <gui>
                <guiname xml:lang="nl">Gewijzigd</guiname>
                <guiname xml:lang="en">Lastmodifieddate</guiname>
                <guiname xml:lang="zh">最后修改时间</guiname>
            </gui>
            <editor>
                <positions>
                    <input>8</input>
                    <list>8</list>
                    <search>8</search>
                </positions>
            </editor>
            <datatype base="lastmodified" xmlns="http://www.mmbase.org/xmlns/datatypes"/>
        </field>
        <field name="creator" state="system">
            <gui>
                <guiname xml:lang="nl">Aangemaakt door</guiname>
                <guiname xml:lang="en">Creator</guiname>
                <guiname xml:lang="zh">创建人</guiname>
            </gui>
            <editor>
                <positions>
                    <input>9</input>
                    <list>9</list>
                    <search>9</search>
                </positions>
            </editor>
            <datatype base="creator" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="lastmodifier" state="system">
            <gui>
                <guiname xml:lang="nl">Gewijzigd door</guiname>
                <guiname xml:lang="en">Lastmodifier</guiname>
                <guiname xml:lang="zh">最后修改人</guiname>
            </gui>
            <editor>
                <positions>
                    <input>10</input>
                    <list>10</list>
                    <search>10</search>
                </positions>
            </editor>
            <datatype base="lastmodifier" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="1024"/>
            </datatype>
        </field>
        <field name="stacktrace">
            <gui>
                <guiname xml:lang="us">stacktrace</guiname>
                <guiname xml:lang="en">stacktrace</guiname>
                <guiname xml:lang="nl">stacktrace</guiname>
            </gui>
            <editor>
                <positions>
                    <input>5</input>
                    <list>5</list>
                    <search>5</search>
                </positions>
            </editor>
            <datatype base="field" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="8192"/>
            </datatype>
        </field>
        <field name="nodetype">
            <descriptions>
                <description xml:lang="nl">NodeType</description>
                <description xml:lang="en">NodeType</description>
                <description xml:lang="zh">所属类型</description>
            </descriptions>
            <gui>
                <guiname xml:lang="nl">NodeType</guiname>
                <guiname xml:lang="en">NodeType</guiname>
                <guiname xml:lang="zh">所属类型</guiname>
            </gui>
            <editor>
                <positions>
                    <input>13</input>
                    <list>13</list>
                    <search>13</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <maxLength value="64"/>
            </datatype>
        </field>
    </fieldlist>
</builder>
