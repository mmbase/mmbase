                <guiname xml:lang="en">User</guiname>
                <guiname xml:lang="zh">用户</guiname>
            </gui>
            <editor>
                <positions>
                    <input>1</input>
                    <list>1</list>
                    <search>1</search>
                </positions>
            </editor>
            <datatype base="line" xmlns="http://www.mmbase.org/xmlns/datatypes">
                <required value="true"/>
                <maxLength value="512"/>
            </datatype>
        </field>
        <field name="name">
            <gui>
                <guiname xml:lang="nl">Naam</guiname>
                <guiname xml:lang="en">Name</guiname>
