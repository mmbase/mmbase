<?xml version="1.0" encoding="UTF-8"?>
<builder extends="object" maintainer="finalist.com" name="favorite" version="1"
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mmbase.org/xmlns/builder http://www.mmbase.org/xmlns/builder.xsd">
    <names>
        <singular xml:lang="nl">favorite</singular>
        <singular xml:lang="en">favorite</singular>
        <singular xml:lang="zh">喜爱</singular>
        <plural xml:lang="nl">favorites</plural>
        <plural xml:lang="en">favorites</plural>
