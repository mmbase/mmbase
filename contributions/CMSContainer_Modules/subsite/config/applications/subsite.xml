<?xml version="1.0"?>
<!DOCTYPE application PUBLIC "-//MMBase/DTD application config 1.1//EN" "http://www.mmbase.org/dtd/application_1_1.dtd">
<application name="subsite" maintainer="finalist.com" version="2" auto-deploy="true">
   <requirements>
      <requires name="contentrepository" maintainer="finalist.com" version="2" type="application"/>
      <requires name="sitemanagement" maintainer="finalist.com" version="1" type="application"/>
   </requirements>

   <neededbuilderlist>
     <!-- These are the application specific builders -->
      <builder maintainer="finalist.com" version="1">subsite</builder>
   </neededbuilderlist>

   <neededreldeflist>
   </neededreldeflist>

   <allowedrelationlist>
      <relation from="page" to="subsite" type="navrel" />
      <relation from="subsite" to="contentchannel" type="related" />
      <relation from="subsite" to="typedef" type="allowrel" />
   </allowedrelationlist>

   <datasourcelist>
      <datasource builder="editwizards" path="subsite/editwizards.xml" />
   </datasourcelist>

   <relationsourcelist>
   </relationsourcelist>

   <contextsourcelist>
   </contextsourcelist>

   <description></description>
   <install-notice></install-notice>
</application>