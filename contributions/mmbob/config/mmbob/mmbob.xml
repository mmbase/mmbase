<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mmbobconfig PUBLIC "-//MMBase/DTD mmbob config 1.0//EN" "http://www.mmbase.org/dtd/mmbobconfig_1_0.dtd">
<mmbobconfig>
  <forums account="admin" password="admin2k" language="en">
    <generatedata role="body" file="generate/body.txt" tokenizer=",!? " />
    <generatedata role="smileys" file="generate/smileys.txt" tokenizer=" \n\r" />
    <generatedata role="punctuation" file="generate/punctuation.txt" tokenizer=" \n\r" />
    <accountcreation type="open" />
    <accountremoval type="open" />
    <loginmode type="open" />
    <logoutmode type="open" />

    <guestreadmode type="open" />
    <guestwritemode type="open" />

    <avatars>
      <upload enable="true"/>
      <gallery enable="true"/>
    </avatars>

    <xslts>
      <postings odd="xslt/posting2xhtmlDark.xslt" even="xslt/posting2xhtmlLight.xslt" />
    </xslts>

    <contactinfo enable="true"/>
    <smileys enable="true"/>
    <privatemessages enable="true"/>
    <postingsperpage value="15"/>

    <forum id="submarine" language="nl">
      <accountcreation type="closed" />
      <accountremoval type="closed" />
      <loginmode type="closed" />
      <logoutmode type="closed" />
      <postarea id="Algemeen" language="nl">
      </postarea>
      <postarea id="Misc" language="nl">
      </postarea>
    </forum>


    <forum id="test" language="nl">
      <guestreadmode type="open" />
      <guestwritemode type="closed" />
      <postarea id="gast gebied" language="nl">
        <guestreadmode type="open" />
        <guestwritemode type="open" />
      </postarea>
      <postarea id="leden gebied" language="nl">
        <guestreadmode type="closed" />
        <guestwritemode type="closed" />
      </postarea>
    </forum>
  </forums>
</mmbobconfig>
