<?xml version="1.0"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<!-- ads -->
<builder version="1" name="ads" maintainer="ilse.nl" extends="object">
    <searchage>9999</searchage>
    <names>
        <singular xml:lang="nl">ad</singular>
        <singular xml:lang="en">ad</singular>
        <plural xml:lang="nl">ads</plural>
        <plural xml:lang="en">ads</plural>
    </names>
    <descriptions>
        <description xml:lang="nl">ad</description>
        <description xml:lang="en">ad</description>
        <description xml:lang="us">ad</description>
    </descriptions>
    <properties/>
    <fieldlist>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Titel</guiname>
                <guiname xml:lang="en">Title</guiname>
                <guitype>string</guitype>
            </gui>
            <editor>
                <positions>
                    <input>1</input>
                    <list>1</list>
                    <search>1</search>
                </positions>
            </editor>
            <db>
                <name>title</name>
                <type size="255" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Tekst</guiname>
                <guiname xml:lang="en">Text</guiname>
                <guitype>field</guitype>
            </gui>
            <editor>
                <positions>
                    <input>2</input>
                    <list>-1</list>
                    <search>2</search>
                </positions>
            </editor>
            <db>
                <name>text</name>
                <type size="1024" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Email</guiname>
                <guiname xml:lang="en">Email</guiname>
                <guitype>string</guitype>
            </gui>
            <editor>
                <positions>
                    <input>3</input>
                    <list>-1</list>
                    <search>3</search>
                </positions>
            </editor>
            <db>
                <name>email</name>
                <type size="255" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">VerloopDatum</guiname>
                <guiname xml:lang="en">ExpireDate</guiname>
                <guitype>eventtime</guitype>
            </gui>
            <editor>
                <positions>
                    <input>11</input>
                    <list>-1</list>
                    <search>11</search>
                </positions>
            </editor>
            <db>
                <name>expiredate</name>
                <type key="false" state="persistent" notnull="false">INTEGER</type>
            </db>
        </field>
    </fieldlist>
</builder>
