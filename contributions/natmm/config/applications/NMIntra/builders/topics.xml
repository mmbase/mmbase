<?xml version="1.0"?>
<!DOCTYPE builder PUBLIC "-//MMBase//DTD builder config 1.1//EN" "http://www.mmbase.org/dtd/builder_1_1.dtd">
<!-- topics -->
<builder version="0" name="topics" maintainer="mmbase.org" extends="object">
    <searchage>9999</searchage>
    <names>
        <singular xml:lang="nl">onderwerp</singular>
        <singular xml:lang="en">topic</singular>
        <singular xml:lang="us">topic</singular>
        <plural xml:lang="nl">onderwerpen</plural>
        <plural xml:lang="en">topics</plural>
        <plural xml:lang="us">topics</plural>
    </names>
    <descriptions>
        <description xml:lang="nl">Onderwerpen van andere objecten, bv onderwerp van een stuk video</description>
        <description xml:lang="en">The topic of a other object, for example the topic of a piece of video</description>
        <description xml:lang="us">The topic of a other object, for example the topic of a piece of video</description>
    </descriptions>
    <properties/>
    <fieldlist>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Titel</guiname>
                <guiname xml:lang="en">Title</guiname>
                <guiname xml:lang="us">Title</guiname>
                <guitype>string</guitype>
            </gui>
            <editor>
                <positions>
                    <input>1</input>
                    <list>1</list>
                    <search>1</search>
                </positions>
            </editor>
            <db>
                <name>title</name>
                <type size="255" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
        <field>
            <descriptions/>
            <gui>
                <guiname xml:lang="nl">Tekst</guiname>
                <guiname xml:lang="en">Text</guiname>
                <guiname xml:lang="us">Text</guiname>
                <guitype>field</guitype>
            </gui>
            <editor>
                <positions>
                    <input>2</input>
                    <list>-1</list>
                    <search>2</search>
                </positions>
            </editor>
            <db>
                <name>description</name>
                <type size="1024" key="false" state="persistent" notnull="false">STRING</type>
            </db>
        </field>
    </fieldlist>
</builder>
